#include "scip/scipbuildflags.h"

/** returns the flags that were used to build SCIP */
const char* SCIPgetBuildFlags(
   void
   )
{
   return " ARCH=@CMAKE_SYSTEM@_@CMAKE_SYSTEM_PROCESSOR@\n COMP=@CMAKE_C_COMPILER_ID@ @CMAKE_C_COMPILER_VERSION@\n BUILD=@CMAKE_BUILD_TYPE@\n";
}
